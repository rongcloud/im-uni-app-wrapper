{
    "compilerOptions": {
        "target": "esnext",
        "lib": ["ESNext", "DOM"],
        "module": "esnext",
        "declaration": true,
        "outDir": "dist",
        "rootDir": "src",
        "strict": true,
        "removeComments": false
    },
    "include": ["src"],
    "exclude": ["node_modules"],
    "typedocOptions": {
        "exclude": "src/uni.d.ts",
        "out": "build/docs",
        "name": "RongCloud-IMWrapper-V2",
        "includeVersion": true,
        "readme": "README-APIDoc.md",
    }
}
